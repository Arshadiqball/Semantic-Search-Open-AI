{
  "name": "semantic-job-matcher",
  "version": "1.0.0",
  "description": "Semantic job matching system using OpenAI embeddings and resume parsing",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "local": "nodemon src/local-server.js",
    "seed": "node src/scripts/seedJobs.js",
    "init-db": "node src/scripts/initDatabase.js",
    "migrate": "node src/scripts/runMigrations.js",
    "migrate:up": "node src/scripts/runMigrations.js up",
    "migrate:down": "node src/scripts/runMigrations.js down"
  },
  "keywords": ["semantic-search", "job-matching", "openai", "resume-parser"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "pgvector": "^0.1.5",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}

